<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>admin_register</servlet-name>
        <servlet-class>admin_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_addtask</servlet-name>
        <servlet-class>admin_addtask</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_login</servlet-name>
        <servlet-class>admin_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_addinstruction</servlet-name>
        <servlet-class>admin_addinstruction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin_register</servlet-name>
        <url-pattern>/admin_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_addtask</servlet-name>
        <url-pattern>/admin_addtask</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_login</servlet-name>
        <url-pattern>/admin_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_addinstruction</servlet-name>
        <url-pattern>/admin_addinstruction</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
